{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "$id": "http://example.com/example.json",
    "type": "object",
    "default": {},
    "title": "Root Schema",
    "required": [
        "handle",
        "name",
        "taxonomy",
        "image",
        "description",
        "maxsize",
        "type"
    ],
    "properties": {
        "handle": {
            "type": "string",
            "default": "",
            "title": "The handle Schema",
            "examples": [
                "Angelfish"
            ]
        },
        "name": {
            "type": "string",
            "default": "",
            "title": "The name Schema",
            "examples": [
                "Angelfish"
            ]
        },
        "taxonomy": {
            "type": "string",
            "default": "",
            "title": "The taxonomy Schema",
            "examples": [
                "Pterophyllum scalare"
            ]
        },
        "image": {
            "type": "string",
            "default": "",
            "title": "The image Schema",
            "examples": [
                "https://upload.wikimedia.org/wikipedia/commons/6/6e/Freshwater_angelfish_biodome.jpg"
            ]
        },
        "description": {
            "type": "string",
            "default": "",
            "title": "The description Schema",
            "examples": [
                "In an enclosed tank habitat, the fish’s territorial, aggressive nature is heightened, so don’t house angelfish with shy species that are intimidated by pushy, boisterous fish.[39]"
            ]
        },
        "maxsize": {
            "type": "string",
            "default": "",
            "title": "The maxsize Schema",
            "examples": [
                "15 cm (5.9 in)"
            ]
        },
        "type": {
            "type": "string",
            "default": "",
            "title": "The type Schema",
            "examples": [
                "South-American-cichlids"
            ]
        }
    },
    "examples": [{
        "handle": "Angelfish",
        "name": "Angelfish",
        "taxonomy": "Pterophyllum scalare",
        "image": "https://upload.wikimedia.org/wikipedia/commons/6/6e/Freshwater_angelfish_biodome.jpg",
        "description": "In an enclosed tank habitat, the fish’s territorial, aggressive nature is heightened, so don’t house angelfish with shy species that are intimidated by pushy, boisterous fish.[39]",
        "maxsize": "15 cm (5.9 in)",
        "type": "South-American-cichlids"
    }]
}